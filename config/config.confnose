## SERVER SETTINGS ##

# Folder to run server from
serverFolder = "server"

# Command to run server
toRun = "java -Xmx1G -jar server.jar --nogui"

## WEBHOOK SETTINGS ##

# Discord Webhook Uri
webhookUri = ""

# If sending webhooks should be enabled (True / False)
webhooks = False

## MISC SETTINGS ##

# Auto STD In - Pipe STD In  (Allow plugins to run commands on the server, but disables normal STD IN)]
autoStdIn = False

# Open a Shell for Pipe
openShell = False